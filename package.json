{
  "name": "ZenView",
  "productName": "ZenView",
  "version": "1.0.0",
  "description": "A Electron app that eases serial data formatting and visualization. ZenView is a powerful Data visualization tool, which was made especially for Zenith's missions.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "install": "electron-rebuild --pre-gyp-fix",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Maíra Canal",
      "url": "https://github.com/mairacanal"
    },
    {
      "name": "Matheus Vieira Gonçalves",
      "url": "https://github.com/Math-42"
    }
  ],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "zenview"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bson": "^4.2.2",
    "electron-squirrel-startup": "^1.0.0",
    "gridstack": "^2.0.2",
    "interactjs": "^1.10.0",
    "javascript-detect-element-resize": "^0.5.3",
    "jquery": "^3.5.1",
    "mathjs": "^7.5.1",
    "n-readlines": "^1.0.0",
    "object-hash": "^2.1.1",
    "plotly.js": "^1.54.7",
    "popper.js": "^1.16.1",
    "serialport": "^9.0.7",
    "tributejs": "^5.1.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "buffer": "^5.7.0",
    "electron": "10.2.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.12.1",
    "eslint-config-google": "^0.14.0"
  }
}
